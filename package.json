{
  "name": "finance-chat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "server:dev": "cd server && nodemon src/index.js",
    "server:start": "cd server && node src/index.js",
    "dev": "concurrently \"npm run serve\" \"npm run server:dev\"",
    "install:all": "npm install && cd server && npm install && cd ../netlify && npm install",
    "netlify:dev": "netlify dev",
    "deploy": "netlify deploy",
    "deploy:prod": "netlify deploy --prod",
    "deploy:init": "netlify init",
    "deploy:build": "npm run build && npm run deploy",
    "deploy:full": "npm run build && npm run deploy:prod",
    "test:api": "node scripts/test-deployment.js",
    "deploy:digbizz": "npm run build && netlify deploy --prod --dir=dist --site=digbizzfinances"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "core-js": "^3.8.3",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "gsap": "^3.13.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.2.0",
    "semver": "^7.7.2",
    "serverless-http": "^3.2.0",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "concurrently": "^8.0.1",
    "dotenv": "^16.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "netlify-cli": "^15.0.0",
    "resolve": "^1.22.10",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
